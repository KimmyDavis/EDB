import {apiSlice} from "../../appState/api/apiSlice"

export const songsApiSlice = apiSlice.injectEndpoints({
  endpoints: (builder) => ({
    createSong: builder.mutation({
      query: (songData) => ({
        query: "/songs",
        method: "POST",
        body: {...songData}
        
      }),
  }),
})
})

export const { useCreateSongMutation } =
  songsApiSlice;
